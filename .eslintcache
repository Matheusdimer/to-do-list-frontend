n      render={(props) =>\n        !session.loggedIn ? (\n          <Component {...props} />\n        ) : (\n          <Redirect to={{ pathname: \"/\", state: { from: props.location } }} />\n        )\n      }\n    />\n  );\n}\n\nexport default function Routes() {\n  const { theme } = useContext(ThemeContext);\n  console.log(theme);\n  return (\n    <BrowserRouter>\n      <GlobalStyle theme={theme} />\n      <AuthProvider>\n        <Switch>\n          <Route path=\"/\" exact={true} component={Home} />\n          <Route path=\"/login\" component={Login} />\n          <Route path=\"/register\" component={Register} />\n          <PrivateRoute path=\"/app\" component={Application} />\n        </Switch>\n      </AuthProvider>\n    </BrowserRouter>\n  );\n}\n",["55","56"],"/home/matheus/GitHub/reactjs-to-do/frontend/src/auth/authenticate.js",[],"/home/matheus/GitHub/reactjs-to-do/frontend/src/contexts/authContext.js",[],["57","58"],"/home/matheus/GitHub/reactjs-to-do/frontend/src/App.js",[],"/home/matheus/GitHub/reactjs-to-do/frontend/src/pages/Register.js",["59"],"/home/matheus/GitHub/reactjs-to-do/frontend/src/pages/Login.js",["60"],"import React from \"react\";\nimport { useState, useContext } from \"react\";\nimport { Link, Redirect } from \"react-router-dom\";\nimport { AuthContext } from '../contexts/authContext';\nimport { ThemeContext } from '../contexts/ThemeContext';\n\nexport default function Login() {\n  const [email, setEmail] = useState();\n  const [password, setPassword] = useState();\n\n  const { session, signIn } = useContext(AuthContext);\n  const { switchTheme } = useContext(ThemeContext);\n\n  document.title = \"Login\"\n\n  function isFieldErr(field) {\n    return session.fieldErr === field ? true : false;\n  }\n\n  return (\n    <div className=\"auth-page\">\n      { session.loggedIn && <Redirect to='/app' /> }\n      <div className=\"loginBox\">\n        <p className=\"label\">E-mail{isFieldErr('email') && <span className='error-message'>Usuário inválido</span>}</p>\n        <input\n          className={`auth-input ${isFieldErr('email') ? 'error' : ''}`}\n          type=\"email\"\n          onChange={({ target }) => setEmail(target.value)}\n        ></input>\n        <p className=\"label\">Senha{isFieldErr('password') && <span className='error-message'>Senha inválida</span>}</p>\n        <input\n          className={`auth-input ${isFieldErr('password') ? 'error' : ''}`}\n          type=\"password\"\n          onChange={({ target }) => setPassword(target.value)}\n        ></input>\n        <button type=\"submit\" className=\"auth-button\" onClick={() => signIn(email, password)}>\n          Login\n        </button>\n        <div className=\"sign-up\">\n          <p>Não possui uma conta? </p>\n          <Link\n            style={{ color: \"#609ff1\", textDecoration: \"none\" }}\n            to=\"/register\"\n          >\n            Cadastre-se\n          </Link>\n        </div>\n      </div>\n    </div>\n  );\n}\n","/home/matheus/GitHub/reactjs-to-do/frontend/src/pages/Application.js",[],"/home/matheus/GitHub/reactjs-to-do/frontend/src/pages/Home.js",[],"/home/matheus/GitHub/reactjs-to-do/frontend/src/auth/register.js",[],"/home/matheus/GitHub/reactjs-to-do/frontend/src/style/GlobalStyle.js",[],"/home/matheus/GitHub/reactjs-to-do/frontend/src/contexts/ThemeContext.js",[],{"ruleId":"61","severity":1,"message":"62","line":9,"column":10,"nodeType":"63","messageId":"64","endLine":9,"endColumn":23},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"65","replacedBy":"69"},{"ruleId":"67","replacedBy":"70"},{"ruleId":"61","severity":1,"message":"71","line":3,"column":20,"nodeType":"63","messageId":"64","endLine":3,"endColumn":29},{"ruleId":"61","severity":1,"message":"72","line":12,"column":11,"nodeType":"63","messageId":"64","endLine":12,"endColumn":22},"no-unused-vars","'ThemeProvider' is defined but never used.","Identifier","unusedVar","no-native-reassign",["73"],"no-negated-in-lhs",["74"